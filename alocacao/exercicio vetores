#include<stdio.h>
#include<stdlib.h>

void identidade(char masc, char fem){
	char *mf=(char*)malloc(2*sizeof(char));
}

int main (void){
	int cont, numero_pessoas;
	char m, f;
	printf("Quantas pessoas participaram do questionario?\n");
	scanf("%d", &numero_pessoas);
	printf("Informe o sexo dos entrevistados: \n");
	for(cont=0; cont<numero_pessoas;cont++){
		identidade(m, f);
		scanf("%s", &identidade);
	}
	
}
